- model_class = Category
%ul#sample_category_tree
  = categories_tree
.page-header
  %h1=t model_class.model_name.human.pluralize(:ru) 
  = link_to t('helpers.links.new'), 
      new_admin_category_path,
      class: '  btn btn-primary'
  .row
    = render 'categories_list', categories: @categories
